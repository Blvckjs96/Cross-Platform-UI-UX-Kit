  "platform": {
    "web": {
      "format": "hex or hsl()",
      "css-var": "--color-{name}",
      "tailwind": "bg-{name}, text-{name}"
    },
    "electron": {
      "format": "hex",
      "note": "Match OS theme preference"
    },
    "swiftui": {
      "format": "Color(.sRGB, red:, green:, blue:)",
      "asset": "ColorAsset in Assets.xcassets",
      "semantic": "Color.primary, Color.secondary, Color.accentColor"
    },
    "react-native": {
      "format": "hex or rgba()",
      "note": "Use Appearance API for dark mode"
    },
    "flutter": {
      "format": "Color(0xAARRGGBB)",
      "semantic": "Theme.of(context).colorScheme.primary"
    }

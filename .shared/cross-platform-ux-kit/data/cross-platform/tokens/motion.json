      "tailwind": "ease",
      "framer": "easeInOut",
      "swiftui": ".easeInOut",
      "flutter": "Curves.ease",
      "use": "General purpose, balanced feel"
    },
    "ease-in": {
      "value": "cubic-bezier(0.4, 0, 1, 1)",
      "css": "ease-in",

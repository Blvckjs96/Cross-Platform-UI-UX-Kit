      "tailwind": "ease-out",
      "framer": "easeOut",
      "swiftui": ".easeOut",
      "flutter": "Curves.easeOut",
      "use": "Entrance animations, elements appearing"
    },
    "ease-in-out": {
      "value": "cubic-bezier(0.4, 0, 0.2, 1)",
      "css": "ease-in-out",
      "tailwind": "ease-in-out",
      "framer": "easeInOut",
      "swiftui": ".easeInOut",
      "flutter": "Curves.easeInOut",

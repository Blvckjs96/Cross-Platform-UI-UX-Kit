    },
    "smooth": {
      "value": "cubic-bezier(0.25, 0.1, 0.25, 1)",
      "css": "cubic-bezier(0.25, 0.1, 0.25, 1)",
      "tailwind": "custom",
      "framer": "[0.25, 0.1, 0.25, 1]",
      "swiftui": ".timingCurve(0.25, 0.1, 0.25, 1)",
      "flutter": "Cubic(0.25, 0.1, 0.25, 1)",
      "use": "Premium feel, subtle transitions"
    }
  },
  "keyframes": {
    "fade-in": {
      "from": { "opacity": 0 },
      "to": { "opacity": 1 },
      "tailwind": "animate-in fade-in",
      "framer": "initial={{ opacity: 0 }} animate={{ opacity: 1 }}"
    },
    "fade-out": {
      "from": { "opacity": 1 },
      "to": { "opacity": 0 },
      "tailwind": "animate-out fade-out",
      "framer": "exit={{ opacity: 0 }}"
    },
    "slide-in-from-top": {
      "from": { "transform": "translateY(-100%)" },
      "to": { "transform": "translateY(0)" },
      "tailwind": "animate-in slide-in-from-top",
      "framer": "initial={{ y: '-100%' }} animate={{ y: 0 }}"
    },
    "slide-in-from-bottom": {
      "from": { "transform": "translateY(100%)" },
      "to": { "transform": "translateY(0)" },
      "tailwind": "animate-in slide-in-from-bottom",
      "framer": "initial={{ y: '100%' }} animate={{ y: 0 }}"
    },
    "slide-in-from-left": {
      "from": { "transform": "translateX(-100%)" },
      "to": { "transform": "translateX(0)" },
      "tailwind": "animate-in slide-in-from-left",
      "framer": "initial={{ x: '-100%' }} animate={{ x: 0 }}"
    },
    "slide-in-from-right": {
      "from": { "transform": "translateX(100%)" },
      "to": { "transform": "translateX(0)" },
      "tailwind": "animate-in slide-in-from-right",
      "framer": "initial={{ x: '100%' }} animate={{ x: 0 }}"
    },
    "zoom-in": {
      "from": { "transform": "scale(0.95)", "opacity": 0 },
      "to": { "transform": "scale(1)", "opacity": 1 },
      "tailwind": "animate-in zoom-in-95",
      "framer": "initial={{ scale: 0.95, opacity: 0 }} animate={{ scale: 1, opacity: 1 }}"
    },
    "zoom-out": {
      "from": { "transform": "scale(1)", "opacity": 1 },
      "to": { "transform": "scale(0.95)", "opacity": 0 },
      "tailwind": "animate-out zoom-out-95",
      "framer": "exit={{ scale: 0.95, opacity: 0 }}"
    },
    "spin": {
      "from": { "transform": "rotate(0deg)" },
      "to": { "transform": "rotate(360deg)" },
      "tailwind": "animate-spin",
      "framer": "animate={{ rotate: 360 }} transition={{ repeat: Infinity, duration: 1 }}"
    },
    "pulse": {
      "0%, 100%": { "opacity": 1 },
      "50%": { "opacity": 0.5 },
      "tailwind": "animate-pulse",
      "framer": "animate={{ opacity: [1, 0.5, 1] }} transition={{ repeat: Infinity }}"
    },
    "bounce": {
      "0%, 100%": { "transform": "translateY(-25%)", "animationTimingFunction": "cubic-bezier(0.8,0,1,1)" },
      "50%": { "transform": "translateY(0)", "animationTimingFunction": "cubic-bezier(0,0,0.2,1)" },
      "tailwind": "animate-bounce",
      "framer": "animate={{ y: [0, -10, 0] }} transition={{ repeat: Infinity }}"
    },
    "shake": {
      "0%, 100%": { "transform": "translateX(0)" },
      "25%": { "transform": "translateX(-5px)" },
      "75%": { "transform": "translateX(5px)" },
      "tailwind": "custom",
      "framer": "animate={{ x: [0, -5, 5, -5, 5, 0] }}"
    },
    "skeleton": {
      "0%": { "backgroundPosition": "200% 0" },
      "100%": { "backgroundPosition": "-200% 0" },
      "tailwind": "animate-pulse bg-gradient-to-r from-muted via-muted/50 to-muted",
      "note": "Shimmer effect for loading placeholders"
    }
  },
  "presets": {
    "button-hover": {
      "duration": "fast",
      "easing": "ease-out",
      "properties": ["background-color", "border-color", "box-shadow"]
    },
    "button-press": {
      "duration": "fastest",
      "easing": "ease-in",
      "transform": "scale(0.98)"
    },
    "modal-enter": {
      "duration": "normal",
      "easing": "ease-out",
      "keyframe": "zoom-in"
    },
    "modal-exit": {
      "duration": "fast",
      "easing": "ease-in",
      "keyframe": "zoom-out"
    },
    "dropdown-enter": {
      "duration": "fast",
      "easing": "ease-out",
      "keyframe": "fade-in"
    },
    "sidebar-toggle": {
      "duration": "relaxed",
      "easing": "ease-in-out",
      "keyframe": "slide-in-from-left"
    },
    "toast-enter": {
      "duration": "relaxed",
      "easing": "spring",
      "keyframe": "slide-in-from-right"
    },
    "page-transition": {
      "duration": "slow",
      "easing": "ease-in-out",
      "keyframe": "fade-in"
    }
  },
  "reduced-motion": {
    "note": "Respect prefers-reduced-motion media query",
    "tailwind": "motion-reduce:transition-none motion-reduce:animate-none",
    "css": "@media (prefers-reduced-motion: reduce) { animation: none; transition: none; }",
    "swiftui": ".transaction { animation: nil }",
    "flutter": "MediaQuery.of(context).disableAnimations",
    "fallback": "Use opacity-only transitions as fallback"
  },
  "platform": {
    "web": {
      "implementation": "CSS transitions, Tailwind animate, Framer Motion",
      "performance": "Use transform and opacity for GPU acceleration"
    },
    "electron": {
      "implementation": "Same as web, respect system reduce-motion",
      "note": "Match native app feel with spring animations"
    },
    "swiftui": {
      "implementation": ".animation(), withAnimation {}, .transition()",
      "spring": ".spring(response: 0.3, dampingFraction: 0.6)"
    },
    "react-native": {
      "implementation": "Animated API, react-native-reanimated",
      "performance": "Use useNativeDriver: true"
    },
    "flutter": {
      "implementation": "AnimatedWidget, AnimationController, implicit animations",
      "curves": "Curves.easeInOut, Curves.bounceOut"
    }
  }
}

    },
    "spring": {
      "value": "cubic-bezier(0.34, 1.56, 0.64, 1)",
      "css": "cubic-bezier(0.34, 1.56, 0.64, 1)",
      "tailwind": "custom",
      "framer": "{ type: 'spring', stiffness: 400, damping: 17 }",
      "swiftui": ".spring(response: 0.3, dampingFraction: 0.6)",
      "flutter": "Curves.elasticOut",

    },
    "bounce": {
      "value": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "css": "cubic-bezier(0.68, -0.55, 0.265, 1.55)",
      "tailwind": "custom",
      "framer": "{ type: 'spring', stiffness: 300, damping: 10 }",
